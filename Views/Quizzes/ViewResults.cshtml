@model IEnumerable<ChurchSystem.Models.StudentQuizResult>
@{
    ViewBag.Title = "ViewResults";
    Layout = "~/Views/Shared/dLayout.cshtml";
}


<br />
<br />
<center>
    <h2>Quiz Attempts/Results </h2>
    <hr style="width:50%" />
</center>

<center>
    <table class="table">
        <tr>
            <th>
                User
            </th>
            <th>
                Score
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.user)
                </td>
                <td>
                    @Html.DisplayFor(modelitem => item.Score)
                </td>
                <td>
                    <a class="btn btn-outline-success" href="@Url.Action("AdminAttemptReview","Quizzes",new {id = item.StudentQuizResultId})">
                        View Attempt
                    </a>
                </td>
            </tr>
        }

    </table>
</center>

<br />
<br />
