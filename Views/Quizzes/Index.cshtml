@model IEnumerable<ChurchSystem.Models.Quiz>

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/dLayout.cshtml";
}


<h2>Quizzes</h2>

<p>
    @Html.ActionLink("Create New Quiz", "Create")
</p>

<table class="table">
    <tr>
        <th>Title</th>
        <th></th>
    </tr>

    @foreach (var quiz in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => quiz.Title)</td>
            <td>@Html.ActionLink("Take", "Take", new { id = quiz.QuizID })</td>
            <td>@Html.ActionLink("Finish", "Finish", new { id = quiz.QuizID })</td>
        </tr>
    }

</table>



@section scripts{





}