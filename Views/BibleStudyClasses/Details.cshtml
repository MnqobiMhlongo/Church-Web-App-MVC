@model ChurchSystem.Models.BibleStudyClasses

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/dLayout.cshtml";
}
<center>
    <img src="~/Content/img/search.gif" height="200" width="200" />
</center>
<center>
    <h2>Bible Study Class</h2>
    <div>
        <hr style="width:50%" />
        <dl class="dl-horizontal">

            <dt>
                Date And Time
            </dt>

            <dd>
                @Html.DisplayFor(model => model.DateTime)
            </dd>

            <dt>
                Description
            </dt>

            <dd>
                @Html.DisplayFor(model => model.classdecription)
            </dd>

            <dt>
                Venue
            </dt>

            <dd>
                @Html.DisplayFor(model => model.venueId)
            </dd>

            <dt>
                Leader
            </dt>

            <dd>
                @Html.DisplayFor(model => model.churchLeader)
            </dd>

        </dl>
    </div>
</center>


<br/>
<br />
<br />
<div class="button">
    <a class="btn btn-outline-primary" href="@Url.Action("CourseSessionsCalender", "BibleStudyClasses", new { CourseId = Model.CourseId })"> Back To Calender</a>
</div>
