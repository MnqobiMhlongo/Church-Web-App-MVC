<center>

    <h3> Memberships </h3>
    <div class="divider-custom">
        <div class="divider-custom-line"></div>
        <div class="divider-custom-icon"><i class="fas fa-cross"></i></div>
        <div class="divider-custom-line"></div>
    </div>
</center>
<div class="row">
    @foreach (var vehicle in Model)
    {
        <div class="col-md-4">
            <div class="card">
                <center>

                    <img src="~/Content/themes/base/images/user-svgrepo-com.svg" class="card-img-top " alt="Vehicle Image" height="120" width="120">
                    <div class="card-body">
                        <p class="card-text">
                            <h5>@vehicle.NameSurname</h5>
                            <strong>Age: </strong>@vehicle.Age <br />
                            <strong>Phone Number: </strong>@vehicle.number<br />
                            <strong>Email: </strong>@vehicle.Email<br />
                            <strong>Gender: </strong>@vehicle.Gender<br />
                            <strong>Marital Status: </strong>@vehicle.MaritalStatus<br />
                        </p>
                    </div>

                </center>
            </div>
        </div>
    }
</div>


